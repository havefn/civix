<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.havefn.civix">

    <uses-permission android:name="android.permission.NFC" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ScreenActivity.StartingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScreenActivity.LoginActivity" />
        <activity android:name=".ScreenActivity.RegistrationActivity" />
        <activity android:name=".ScreenActivity.DataEntryActivity" />
        <activity android:name=".ScreenActivity.MainMenuActivity" />
        <activity
            android:name=".Dummyu"
            android:exported="true" />
        <activity android:name=".NavBarActivity"
            android:exported="true"></activity>
        <uses-permission android:name="android.permission.NFC" />
        <uses-sdk android:minSdkVersion="14" />
        <uses-feature android:name="android.hardware.nfc" android:required="false"/>
        <activity android:name=".ScreenActivity.NFCValidationActivity"></activity>
    </application>

</manifest>